sample2 <- rnorm(n, mean = means, sd = sqrt(vars[2]))
sample3 <- rnorm(n, mean = means, sd = sqrt(vars[3]))
data = data.frame(sample1 = sample1, sample2 = sample2, sample3 = sample3)
return(data)
}
